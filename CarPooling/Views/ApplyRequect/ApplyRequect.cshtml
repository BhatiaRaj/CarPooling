@model CarPooling.Models.ApplyRequect

@{
    ViewData["Title"] = "ApplyRequest";
}

<div class="container" style="margin-top:20px;">
    <form>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label>
                    User Name:
                </label>
                <input type="text" class="form-control" id="UserName" placeholder="User Name">
            </div>
            <div class="form-group col-md-6">
                <label>
                    PS No.
                </label>
                <input type="text" class="form-control" id="PsNo" placeholder="PS No.">
            </div>
            <div class="form-group col-md-6">
                <label>
                    Pick Up Point:
                </label>
                <input type="text" class="form-control" id="PickUpPoint" placeholder="Pick Up Point">
            </div>
            <div class="form-group col-md-6">
                <label>
                    Mobile No:
                </label>
                <input type="number" class="form-control" id="MobNo" placeholder="Mobile No.">
            </div>
            <div class="form-group col-md-6">
                <label>
                    Time
                </label>
                <input type="text" class="form-control" id="time">
            </div>
            <div class="form-group col-md-6">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </form>
</div>